@model Metadados.Models.Objeto

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>



<h3>Tem certeza que deseja deletar este objeto?</h3>
<div>
    <h4>Objeto</h4>
    <hr/>
    <dl class="row">
      
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.nom_objeto)
            </dd>
       
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="sky_objeto_configuracao" />
        <input type="submit" value="Delete" class="btn btn-danger" /> 
        <a asp-action="Index">Back to List</a>
    </form>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}