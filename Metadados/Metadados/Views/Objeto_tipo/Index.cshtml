@model IEnumerable<Metadados.Models.Objeto_tipo>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.nom_objeto_tipo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nom_objeto_tipo)
            </td>
         

            <td>
                <a class="btn btn-primary" asp-action="Edit" asp-route-id="@item.sky_objeto_tipo">Edit</a>
                <a class="btn btn-primary" asp-action="Delete" asp-route-id="@item.sky_objeto_tipo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
<input class="btn btn-primary" type="button" value="Create" onclick="location.href='@Url.Action("Create", "Objeto_tipo")'" />
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    
}